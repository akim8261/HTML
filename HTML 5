import random
import string

def generate_passwords(full_name, birth_date, num_passwords=10):
    """–ì–µ–Ω–µ—Ä–∏—Ä—É–µ—Ç 10 —É–Ω–∏–∫–∞–ª—å–Ω—ã—Ö –ø–∞—Ä–æ–ª–µ–π –Ω–∞ –æ—Å–Ω–æ–≤–µ –§–ò–û –∏ –¥–∞—Ç—ã —Ä–æ–∂–¥–µ–Ω–∏—è."""
    
    # –û—á–∏—â–∞–µ–º –∏ —Ä–∞–∑–¥–µ–ª—è–µ–º –∏–º—è
    name_parts = full_name.replace(" ", "").lower()
    
    # –ü—Ä–µ–æ–±—Ä–∞–∑—É–µ–º –¥–∞—Ç—É —Ä–æ–∂–¥–µ–Ω–∏—è (–æ—Å—Ç–∞–≤–ª—è–µ–º —Ç–æ–ª—å–∫–æ —Ü–∏—Ñ—Ä—ã)
    birth_digits = ''.join(filter(str.isdigit, birth_date))
    
    # –°–∏–º–≤–æ–ª—ã –¥–ª—è –≥–µ–Ω–µ—Ä–∞—Ü–∏–∏
    special_chars = "!@#$%^&*"
    
    passwords = set()  # –ò—Å–ø–æ–ª—å–∑—É–µ–º –º–Ω–æ–∂–µ—Å—Ç–≤–æ, —á—Ç–æ–±—ã –∏–∑–±–µ–∂–∞—Ç—å –¥—É–±–ª–∏–∫–∞—Ç–æ–≤
    
    while len(passwords) < num_passwords:
        # –ë–µ—Ä–µ–º —Å–ª—É—á–∞–π–Ω—ã–µ —á–∞—Å—Ç–∏ –∏–º–µ–Ω–∏ –∏ –¥–∞—Ç—ã
        part1 = "".join(random.sample(name_parts, min(5, len(name_parts))))
        part2 = "".join(random.sample(birth_digits, min(4, len(birth_digits))))
        part3 = random.choice(special_chars) + random.choice(string.ascii_letters)
        
        # –°–æ–∑–¥–∞–µ–º –ø–∞—Ä–æ–ª—å –∏ –¥–æ–±–∞–≤–ª—è–µ–º –≤ —Å–ø–∏—Å–æ–∫
        password = part1 + part2 + part3
        passwords.add(password)
    
    return list(passwords)

# --- –í–≤–æ–¥ –¥–∞–Ω–Ω—ã—Ö –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª–µ–º ---
full_name = input("–í–≤–µ–¥–∏—Ç–µ –§–ò–û: ")
birth_date = input("–í–≤–µ–¥–∏—Ç–µ –¥–∞—Ç—É —Ä–æ–∂–¥–µ–Ω–∏—è (–¥–¥.–º–º.–≥–≥–≥–≥): ")

# –ì–µ–Ω–µ—Ä–∞—Ü–∏—è –ø–∞—Ä–æ–ª–µ–π
passwords = generate_passwords(full_name, birth_date)

# –í—ã–≤–æ–¥ –ø–∞—Ä–æ–ª–µ–π
print("\nüîê –°–≥–µ–Ω–µ—Ä–∏—Ä–æ–≤–∞–Ω–Ω—ã–µ –ø–∞—Ä–æ–ª–∏:")
for i, pwd in enumerate(passwords, 1):
    print(f"{i}. {pwd}")
